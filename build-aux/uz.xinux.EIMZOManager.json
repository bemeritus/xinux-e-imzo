{
    "id" : "uz.xinux.EIMZOManager",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "E-IMZO-Manager",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--talk-name=org.freedesktop.secrets",
        "--filesystem=/media"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "env" : {
            "CARGO_HOME": "/run/build/E-IMZO-Manager/cargo"
        }
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name" : "E-IMZO-Manager",
            "run-tests" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://github.com/xinux-org/e-imzo/releases/download/0.1.0/E-IMZO-Manager-0.1.0.tar.xz", 
                     "sha256" : "87ac0e9ba283c4f382db410da4ae158188dc4e7db429e7428dd4f516fb78a4d5"
                }
            ]
        }
    ]
}
